<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">


<bean id = "mathCheatCodeObjectValue" class = "com.application.demo.MathCheatCode"></bean>
<bean id = "javaCheatCodeObjectValue" class = "com.application.demo.JavaCheatCode"></bean>

<bean id = "student" class="com.application.demo.Student">
